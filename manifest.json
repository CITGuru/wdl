{
  "name": "WDL",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Wistia Video Downloader",
  "homepage_url": "http://github.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "options/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "page": "bg/background.html",
    "persistent": false
    },
  "permissions": [
    "notifications",
    "downloads",
    "activeTab",
    "tabs", 
    "<all_urls>",
    "http://*/*", 
    "https://*/*"
  ]
}